<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="spa_order_sequence" model="ir.sequence">
            <field name="name">Spa Order Sequence</field>
            <field name="code">spa.order.sequence</field>
            <field name="prefix">SPA/%(year)s/%(month)s/%(day)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field eval="False" name="use_date_range" />
            <field name="padding">3</field>
        </record>

    </data>
</odoo>